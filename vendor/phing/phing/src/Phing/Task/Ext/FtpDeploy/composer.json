{
    "name": "phing/task-ftpdeploy",
    "description": "Deploys a set of files to a remote FTP server.",
    "license": "LGPL-3.0-only",
    "type": "phing-extension",
    "authors": [
        {
            "name": "Siad Ardroumli",
            "email": "siad.ardroumli@gmail.com"
        }
    ],
    "require": {
        "php": ">= 7.1"
    },
    "require-dev": {
        "phing/phing": "3.0.x-dev",
        "pear/pear-core-minimal": "~1.10.10",
        "pear/net_ftp": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Phing\\Task\\Ext\\FtpDeploy\\": ""
        }
    },
    "extra": {
        "phing-custom-taskdefs": {
            "ftpdeploy": "Phing\\Task\\Ext\\FtpDeploy\\FtpDeployTask"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
