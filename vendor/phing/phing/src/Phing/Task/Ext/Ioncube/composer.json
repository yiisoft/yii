{
  "name": "phing/task-ioncube",
  "description": "The Ioncube tasks executes ionCube commands.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "LGPL-3.0-only",
  "type": "phing-extension",
  "authors": [
    {
      "name": "Siad Ardroumli",
      "email": "siad.ardroumli@gmail.com"
    }
  ],
  "require": {
    "php": ">= 7.1"
  },
  "require-dev": {
    "phing/phing": "3.0.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Phing\\Task\\Ext\\Ioncube\\": ""
    }
  },
  "extra": {
    "phing-custom-taskdefs": {
      "ioncubeencoder": "Phing\\Task\\Ext\\Ioncube\\IoncubeEncoderTask",
      "ioncubelicense": "Phing\\Task\\Ext\\Ioncube\\IoncubeLicenseTask"
    }
  }
}
