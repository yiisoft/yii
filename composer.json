	"require": {
		"php": "^7.2 || ^7.3 || ^7.4",
		"php-http/message": "^1.5"
	},
	"replace": {
		"yiisoft/yii": ">1.1.21 <2"
	},
	"conflict": {
		"yiisoft/yii": ">1.1.21 <2"
	},
	"require-dev": {
		"localheinz/composer-normalize": "^1.3",
		"phpseclib/mcrypt_compat": "^1.0",
		"phpstan/phpstan": "^0.12.0",
		"phpunit/phpunit": "^7.0",
		"roave/security-advisories": "dev-master",
		"vimeo/psalm": "^3.6",
		"zendframework/zend-diactoros": "^2.1"
	},
	"suggest": {
		"ext-mcrypt": "Required by encrypt and decrypt methods of CSecurityManager",
		"phpseclib/mcrypt_compat": "To provide mcrypt polyfill when extension is unavailable"
	},
	"config": {
		"discard-changes": true,
		"sort-packages": true
	},
	"autoload": {
		"classmap": [
			"framework/base/",
			"framework/caching/",
			"framework/cli/commands/",
			"framework/collections/",
			"framework/console/",
			"framework/db/",
			"framework/i18n/",
			"framework/logging/",
			"framework/test/",
			"framework/utils/",
			"framework/validators/",
			"framework/web/",
			"framework/YiiBase.php",
			"framework/zii/",
			"framework/yii.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"yii1\\tests\\": "tests/"
		},
		"classmap": [
			"tests/TestApplication.php"
		]
	},
	"bin": [
		"framework/yiic"
	],
	"support": {
		"issues": "https://github.com/yiisoft/yii/issues?state=open",
		"forum": "http://www.yiiframework.com/forum/",
		"wiki": "http://www.yiiframework.com/wiki/",
		"irc": "irc://irc.freenode.net/yii",
		"source": "https://github.com/yiisoft/yii"
	}
}
