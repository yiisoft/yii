	"bin": [
		"framework/yiic"
	],
	"require": {
		"php": "^7.1||^7.2||^7.3",
		"php-http/message": "^1.5"
	},
	"suggest": {
		"ext-mcrypt": "Required by encrypt and decrypt methods of CSecurityManager",
		"phpseclib/mcrypt_compat": "To provide mcrypt polyfill when extension is unavailable"
	},
	"autoload": {
		"classmap" : [
			"framework/base/",
			"framework/caching/",
			"framework/cli/commands/",
			"framework/collections/",
			"framework/console/",
			"framework/db/",
			"framework/i18n/",
			"framework/logging/",
			"framework/test/",
			"framework/utils/",
			"framework/validators/",
			"framework/web/",
			"framework/YiiBase.php",
			"framework/zii/",
			"framework/yii.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestApplication.php"
		],
		"psr-4": {
			"yii1\\tests\\": "tests/"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "~4.8.34",
		"phpunit/phpunit-selenium": "~1.4.0",
		"phing/phing": "2.*",
		"phpseclib/mcrypt_compat": "^1.0",
		"pear/archive_tar": "~1.4.6",
		"zendframework/zend-diactoros": "^2.1"
	}
}
